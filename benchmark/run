#!/bin/bash -l

#SBATCH -q debug
#SBATCH -N 1
#SBATCH --mail-type=all
#SBATCH --mail-user=gguidi@berkeley.edu
#SBATCH -C haswell
#SBATCH -L SCRATCH
#SBATCH -t 30:00

for (( i = 0; i <= 9; i++ ))      ### Outer for loop ###
do
    echo 'band 32' >> result-bmc

    for (( x = 10; x <= 100; x+10 )) ### Inner for loop ###
    do
            echo 'iter ' $i ' x ' $x >> xavier-bmc
            ./test $x 32 >> xavier-bmc
    done

  echo "" #### print the new line ###
done

for (( i = 0; i <= 9; i++ ))      ### Outer for loop ###
do
    echo 'band 64' >> xavier-bmc

    for (( x = 10; x <= 100; x+10 )) ### Inner for loop ###
    do
            echo 'iter ' $i ' x ' $x >> xavier-bmc
            ./test $x 64 >> xavier-bmc
    done

  echo "" #### print the new line ###
done

for (( i = 0; i <= 9; i++ ))      ### Outer for loop ###
do
    echo 'band 128' >> xavier-bmc

    for (( x = 10; x <= 100; x+10 )) ### Inner for loop ###
    do
            echo 'iter ' $i ' x ' $x >> xavier-bmc
            ./test $x 128 >> xavier-bmc
    done

  echo "" #### print the new line ###
done

for (( i = 0; i <= 9; i++ ))      ### Outer for loop ###
do
    echo 'band 256' >> xavier-bmc

    for (( x = 10; x <= 100; x+10 )) ### Inner for loop ###
    do
            echo 'iter ' $i ' x ' $x >> xavier-bmc
            ./test $x 256 >> xavier-bmc
    done

  echo "" #### print the new line ###
done

for (( i = 0; i <= 9; i++ ))      ### Outer for loop ###
do
    echo 'band 512' >> xavier-bmc

    for (( x = 10; x <= 100; x+10 )) ### Inner for loop ###
    do
            echo 'iter ' $i ' x ' $x >> xavier-bmc
            ./test $x 512 >> xavier-bmc
    done

  echo "" #### print the new line ###
done